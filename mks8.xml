				</attacherJoint>
				<sounds>
					<attach template="ATTACH_01" linkNode="0>" />
				</sounds>
            </attacherJointConfiguration>
        </attacherJointConfigurations>
    </attacherJoints>

    <licensePlates>
        <licensePlate node="licensePlateBack" position="BACK" preferedType="ELONGATED" placementArea="0.06 0.26 0.09 0.26"/>
    </licensePlates>

    <lights>
